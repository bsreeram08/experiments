{
    "name": "async-throttle",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/async-throttle/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": ["{workspaceRoot}/dist/packages/async-throttle"],
            "options": {
                "commands": [
                    "bunup",
                    "mkdir -p ../../dist/packages/async-throttle",
                    "cp -r dist/* ../../dist/packages/async-throttle/",
                    "cp README.md package.json ../../dist/packages/async-throttle/ 2>/dev/null || true"
                ],
                "cwd": "packages/async-throttle",
                "parallel": false
            }
        },
        "build:watch": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bunup --watch",
                "cwd": "packages/async-throttle"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"]
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "bun test",
                "cwd": "packages/async-throttle"
            }
        },
        "test:jest": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/async-throttle/jest.config.ts"
            }
        }
    }
}
